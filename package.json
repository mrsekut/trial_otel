{
  "name": "trial_otel",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "typecheck": "bunx tsc --noEmit",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "check": "bun run typecheck && bun run format && bun run test",
    "jaeger:up": "docker compose up -d",
    "jaeger:down": "docker compose down",
    "jaeger:logs": "docker compose logs -f jaeger",
    "dev": "bun --hot ./src/index.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "prettier": "^3.6.2",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@effect/opentelemetry": "^0.56.1",
    "@eslint/js": "^9.30.1",
    "@hono/graphql-server": "^0.6.2",
    "@hono/otel": "^0.2.2",
    "@opentelemetry/auto-instrumentations-node": "^0.62.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.202.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.203.0",
    "@opentelemetry/instrumentation-graphql": "^0.50.0",
    "@opentelemetry/sdk-metrics": "^2.0.1",
    "@opentelemetry/sdk-node": "^0.202.0",
    "@opentelemetry/sdk-trace-base": "^2.0.1",
    "@opentelemetry/sdk-trace-node": "^2.0.1",
    "@opentelemetry/sdk-trace-web": "^2.0.1",
    "effect": "^3.17.6",
    "graphql": "^16.11.0",
    "hono": "^4.8.4"
  }
}
